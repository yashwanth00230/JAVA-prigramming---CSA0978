import java.util.Scanner;
class A extends Thread
{
    public void run()
    {
        int n=5;
        for(int i=1;i<=n;i++)
        {
            System.out.println(n+" X "+i+" = "+(n*i));
        }
    }
}
class B extends Thread
{
    public void run()
    {
        int n=10;
        for(int i=1;i<=n;i++)
        {
            System.out.println(n+" X "+i+" = "+(n*i));
        }
    }
}
public class ak
{
    public static void main(String[] args)
    {
        Scanner input=new Scanner(System.in);
        A threadA=new A();
        B threadB=new B();
        threadA.start();
        threadB.start();
    }
}


OUTPUT:-
10 X 1 = 10
10 X 2 = 20
5 X 1 = 5
10 X 3 = 30
5 X 2 = 10
10 X 4 = 40
5 X 3 = 15
10 X 5 = 50
5 X 4 = 20
10 X 6 = 60
5 X 5 = 25
10 X 7 = 70
10 X 8 = 80
10 X 9 = 90
10 X 10 = 100
